<main class="floor-index">
  <div class="row">
    <div class="col-xl-3 col-lg-4 col-12 tabs">
      <%= render partial: 'floor_select_panel'%>
    </div>
    <div class="col-12 col-xl-9 col-lg-8 tab-content" id="floors-tabContent">
      <% @floors.each do |floor| %>
        <%= render partial: 'floor_pane', locals: { floor: floor } %>
      <% end %>
    </div>
  </div>
</main>